@model ViewModel

@{
    Layout = "~/Views/Shared/Email/Layout.cshtml";
}

@await Html.PartialAsync("Email/Main")

@await Html.PartialAsync("Email/FooterProduct")

@section scripts {
    <script src="~/js/email/headers.js"></script>

    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestHeaders,
        });
    </script>
}