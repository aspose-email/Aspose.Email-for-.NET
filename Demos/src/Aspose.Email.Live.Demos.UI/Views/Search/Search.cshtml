@model  ViewModel

@{
    Layout = "~/Views/Shared/Layout.cshtml";
}

@Html.Partial("Main")

@Html.Partial("FooterProduct", Model)

@section scripts {
    <script type="text/javascript">
        var o = @Html.Raw(Model.JSOptions);
        $.extend(o, {
            Method: requestSearch,
            validationSearchMessage: "@Html.Raw(Model.Resources["SearchQueryErrorMessage"])"
        });
    </script>
}
